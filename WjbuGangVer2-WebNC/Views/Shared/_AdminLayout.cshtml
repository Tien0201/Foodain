<!DOCTYPE html>
<html lang="en">

<head>
    <title>Trang Admin</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="Trang Admin" />
    <meta name="keywords" content="Trang Admin" />
    <meta name="author" content="codedthemes" />
    @Scripts.Render("~/bundles/jquery")
    <!-- Favicon icon -->
    <link rel="icon" href="~/AdminPages/assets/images/favicon.ico" type="image/x-icon">
    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500" rel="stylesheet">
    <!-- waves.css -->
    <link rel="stylesheet" href="~/AdminPages/assets/pages/waves/css/waves.min.css" type="text/css" media="all">
    <!-- Required Fremwork -->
    <link rel="stylesheet" type="text/css" href="~/AdminPages/assets/css/bootstrap/css/bootstrap.min.css">
    <!-- waves.css -->
    <link rel="stylesheet" href="~/AdminPages/assets/pages/waves/css/waves.min.css" type="text/css" media="all">
    <!-- themify icon -->
    <link rel="stylesheet" type="text/css" href="~/AdminPages/assets/icon/themify-icons/themify-icons.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" type="text/css" href="~/AdminPages/assets/icon/font-awesome/css/font-awesome.min.css">
    <!-- scrollbar.css -->
    <link rel="stylesheet" type="text/css" href="~/AdminPages/assets/css/jquery.mCustomScrollbar.css">
    <!-- am chart export.css -->
    <link rel="stylesheet" href="https://www.amcharts.com/lib/3/plugins/export/export.css" type="text/css" media="all" />
    <!-- Style.css -->
    <link rel="stylesheet" type="text/css" href="~/AdminPages/assets/css/style.css">
    <script src="~/Content/JAVA/showImagePreview.js"></script>
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
</head>

<body>

    <!-- Pre-loader start -->
    <div class="theme-loader">
        <div class="loader-track">
            <div class="preloader-wrapper">
                <div class="spinner-layer spinner-blue">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
                <div class="spinner-layer spinner-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>

                <div class="spinner-layer spinner-yellow">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>

                <div class="spinner-layer spinner-green">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Pre-loader end -->
    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
            <nav class="navbar header-navbar pcoded-header">
                <div class="navbar-wrapper">
                    <div class="navbar-logo">
                        <a class="mobile-menu waves-effect waves-light" id="mobile-collapse" href="#!">
                            <i class="ti-menu"></i>
                        </a>
                        <div class="mobile-search waves-effect waves-light">
                            <div class="header-search">
                                <div class="main-search morphsearch-search">
                                    <div class="input-group">
                                        <span class="input-group-addon search-close"><i class="ti-close"></i></span>
                                        <input id="Search" type="text" class="form-control" placeholder="Enter Keyword">
                                        <span class="input-group-addon search-btn"><i class="ti-search"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <a href="index.html">
                            <img class="img-fluid" src="~/AdminPages/assets/images/logo.png" alt="Theme-Logo" />
                        </a>
                        <a class="mobile-options waves-effect waves-light">
                            <i class="ti-more"></i>
                        </a>
                    </div>

                    <div class="navbar-container container-fluid">
                        <ul class="nav-left">
                            <li>
                                <div class="sidebar_toggle"><a href="javascript:void(0)"><i class="ti-menu"></i></a></div>
                                <input type="text" class="form-control" id="NewSearch" style="margin-top: 10px; width: 400px"/>
                            </li>
                            <li class="header-search">
                                <div class="main-search morphsearch-search">
                                    <div class="input-group">
                                        <span class="input-group-addon search-close"><i class="ti-close"></i></span>
                                        <input type="text" class="form-control" id="Search">
                                        <span class="input-group-addon search-btn"><i class="ti-search"></i></span>
                                    </div>
                                </div>
                            </li>
                            <li>
                                <a href="#!" onclick="javascript:toggleFullScreen()" class="waves-effect waves-light">
                                    <i class="ti-fullscreen"></i>
                                </a>
                            </li>
                        </ul>
                        <ul class="nav-right">
                            <li class="header-notification">
                                <a href="#!" class="waves-effect waves-light">
                                    <i class="ti-bell"></i>
                                    <span class="badge bg-c-red"></span>
                                </a>
                                <ul class="show-notification">
                                    <li>
                                        <h6>Notifications</h6>
                                        <label class="label label-danger">New</label>
                                    </li>
                                    <li class="waves-effect waves-light">
                                        <div class="media">
                                            <img class="d-flex align-self-center img-radius" src="~/Content/Images/user.png" alt="Generic placeholder image">
                                            <div class="media-body">
                                                <h5 class="notification-user">@Session["Admin_Name"]</h5>
                                                <p class="notification-msg">Lorem ipsum dolor sit amet, consectetuer elit.</p>
                                                <span class="notification-time">30 minutes ago</span>
                                            </div>
                                        </div>
                                    </li>
                                    <li class="waves-effect waves-light">
                                        <div class="media">
                                            <img class="d-flex align-self-center img-radius" src="~/AdminPages/assets/images/avatar-4.jpg" alt="Generic placeholder image">
                                            <div class="media-body">
                                                <h5 class="notification-user">Joseph William</h5>
                                                <p class="notification-msg">Lorem ipsum dolor sit amet, consectetuer elit.</p>
                                                <span class="notification-time">30 minutes ago</span>
                                            </div>
                                        </div>
                                    </li>
                                    <li class="waves-effect waves-light">
                                        <div class="media">
                                            <img class="d-flex align-self-center img-radius" src="~/AdminPages/assets/images/avatar-3.jpg" alt="Generic placeholder image">
                                            <div class="media-body">
                                                <h5 class="notification-user">Sara Soudein</h5>
                                                <p class="notification-msg">Lorem ipsum dolor sit amet, consectetuer elit.</p>
                                                <span class="notification-time">30 minutes ago</span>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                            <li class="user-profile header-notification">
                                <a href="#!" class="waves-effect waves-light">
                                    <img src="~/Content/Images/user.png" class="img-radius" alt="User-Profile-Image">
                                    <span>@Session["Admin_Name"]</span>
                                    <i class="ti-angle-down"></i>
                                </a>
                                <ul class="show-notification profile-notification">
                                    <li class="waves-effect waves-light">
                                        <a href="#!">
                                            <i class="ti-settings"></i> Cài đặt
                                        </a>
                                    </li>
                                    <li class="waves-effect waves-light">
                                        <a href="user-profile.html">
                                            <i class="ti-user"></i> Thông tin
                                        </a>
                                    </li>


                                    <li class="waves-effect waves-light">
                                        <a href="auth-normal-sign-in.html">
                                            <a href="~/Account/Adminlogout" onClick="reload()"><i class="ti-layout-sidebar-left"></i>Đăng xuất</a>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>

            <nav class="pcoded-navbar">
                <div class="sidebar_toggle"><a href="#"><i class="icon-close icons"></i></a></div>
                <div class="pcoded-inner-navbar main-menu">
                    <div class="">
                        <div class="main-menu-header">
                            <img class="img-80 img-radius" src="~/Content/Images/user.png" alt="User-Profile-Image">
                            <div class="user-details">
                                <span id="more-details">@Session["Admin_Name"]<i class="fa fa-caret-down"></i></span>
                            </div>
                        </div>

                        <div class="main-menu-content">
                            <ul>
                                <li class="more-details">
                                    <a href="user-profile.html"><i class="ti-user"></i>View Profile</a>
                                    <a href="#!"><i class="ti-settings"></i>Settings</a>
                                    <a href="~/Account/Adminlogout" onClick="reload()"><i class="ti-layout-sidebar-left"></i>Đăng xuất</a>
                                </li>
                            </ul>
                        </div>
                    </div>

                    <ul class="pcoded-item pcoded-left-item">
                        <li class="active">
                            <a href="index.html" class="waves-effect waves-dark">
                                <span class="pcoded-mtext" data-i18n="nav.dash.main">Dashboard</span>
                                <span class="pcoded-mcaret"></span>
                            </a>
                        </li>

                        <!--menu-->
                        <li class="pcoded-hasmenu">
                            <a href="javascript:void(0)" class="waves-effect waves-dark">
                                <span class="pcoded-micon"><i class="ti-layout-grid2-alt"></i></span>
                                <span class="pcoded-mtext" data-i18n="nav.basic-components.main">Quản lý mặt hàng</span>
                                <span class="pcoded-mcaret"></span>
                            </a>
                            <!--components-->
                            <ul class="pcoded-submenu">
                                <li class=" ">
                                    <a href="accordion.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Thêm", "Create", "LoaiMH")
                                    </a>
                                </li>
                                <li class=" ">
                                    <a href="breadcrumb.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Xem Chi Tiết", "Index", "LoaiMH")
                                    </a>
                                </li>
                            </ul>

                            <!--menu-->
                        <li class="pcoded-hasmenu">
                            <a href="javascript:void(0)" class="waves-effect waves-dark">
                                <span class="pcoded-micon"><i class="ti-layout-grid2-alt"></i></span>
                                <span class="pcoded-mtext" data-i18n="nav.basic-components.main">Quản lý sản phẩm</span>
                                <span class="pcoded-mcaret"></span>
                            </a>
                            <!--components-->
                            <ul class="pcoded-submenu">
                                <li class=" ">
                                    <a href="accordion.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Thêm Sản Phẩm", "Create", "MatHang")
                                    </a>
                                </li>
                                <li class=" ">
                                    <a href="breadcrumb.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Chi Tiết Sản Phẩm", "Index", "MatHang")
                                    </a>
                                </li>
                            </ul>


                            <!--menu-->
                        <li class="pcoded-hasmenu">
                            <a href="javascript:void(0)" class="waves-effect waves-dark">
                                <span class="pcoded-micon"><i class="ti-layout-grid2-alt"></i></span>
                                <span class="pcoded-mtext" data-i18n="nav.basic-components.main">Quản lý tài khoản</span>
                                <span class="pcoded-mcaret"></span>
                            </a>
                            <!--components-->
                            <ul class="pcoded-submenu">
                                <li class=" ">
                                    <a href="breadcrumb.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Danh sách tài khoản", "Index", "AccountEntity")
                                    </a>
                                </li>
                            </ul>
                        </li>


                        <!--menu-->
                        <li class="pcoded-hasmenu">
                            <a href="javascript:void(0)" class="waves-effect waves-dark">
                                <span class="pcoded-micon"><i class="ti-layout-grid2-alt"></i></span>
                                <span class="pcoded-mtext" data-i18n="nav.basic-components.main">Quản lý đơn hàng</span>
                                <span class="pcoded-mcaret"></span>
                            </a>
                            <!--components-->
                            <ul class="pcoded-submenu">
                                <li class=" ">
                                    <a href="breadcrumb.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Danh sách đơn hàng", "Index", "QuanlyHoaDon")
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <!--menu-->
                        <li class="pcoded-hasmenu">
                            <a href="javascript:void(0)" class="waves-effect waves-dark">
                                <span class="pcoded-micon"><i class="ti-layout-grid2-alt"></i></span>
                                <span class="pcoded-mtext" data-i18n="nav.basic-components.main">Quản lý voucher</span>
                                <span class="pcoded-mcaret"></span>
                            </a>
                            <!--components-->
                            <ul class="pcoded-submenu">
                                <li class=" ">
                                    <a href="accordion.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Thêm Voucher", "Create", "Voucher")
                                    </a>
                                </li>
                                <li class=" ">
                                    <a href="breadcrumb.html" class="waves-effect waves-dark">
                                        @Html.ActionLink("Danh Sách Voucher", "Index", "Voucher")
                                    </a>
                                </li>
                            </ul>

                    </ul>
            </nav>
            <div class="pcoded-content">
                <!-- Page-header start -->
                <div class="page-header">
                    <div class="page-block">
                        <div class="row align-items-center">
                            <div class="col-md-8">
                                <div class="page-header-title" style="margin-top:31.2px">
                                    <h5 class="m-b-10">Dashboard</h5>
                                    <h3 class="m-b-0">Wjbu Gang Admin</h3>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <ul class="breadcrumb-title">
                                    <li class="breadcrumb-item">
                                        <a href="index.html"> <i class="fa fa-home"></i> </a>
                                    </li>
                                    <li class="breadcrumb-item">
                                        <a href="#!">Dashboard</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="main-body" style="margin: 50px 0px 0px 50px">
                    @RenderBody()
                    <div class="page-body">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/Content/JAVA/showImagePreview.js"></script>
    <script type="text/javascript" src="~/AdminPages/assets/js/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/AdminPages/assets/js/jquery-ui/jquery-ui.min.js "></script>
    <script type="text/javascript" src="~/AdminPages/assets/js/popper.js/popper.min.js"></script>
    <script type="text/javascript" src="~/AdminPages/assets/js/bootstrap/js/bootstrap.min.js "></script>
    <script type="text/javascript" src="~/AdminPages/assets/pages/widget/excanvas.js "></script>
    <!-- waves js -->
    <script src="~/AdminPages/assets/pages/waves/js/waves.min.js"></script>
    <!-- jquery slimscroll js -->
    <script type="text/javascript" src="~/AdminPages/assets/js/jquery-slimscroll/jquery.slimscroll.js "></script>
    <!-- modernizr js -->
    <script type="text/javascript" src="~/AdminPages/assets/js/modernizr/modernizr.js "></script>
    <!-- slimscroll js -->
    <script type="text/javascript" src="~/AdminPages/assets/js/SmoothScroll.js"></script>
    <script src="~/AdminPages/assets/js/jquery.mCustomScrollbar.concat.min.js "></script>
    <!-- Chart js -->
    <script type="text/javascript" src="~/AdminPages/assets/js/chart.js/Chart.js"></script>
    <!-- amchart js -->
    <script src="https://www.amcharts.com/lib/3/amcharts.js"></script>
    <script src="~/AdminPages/assets/pages/widget/amchart/gauge.js"></script>
    <script src="~/AdminPages/assets/pages/widget/amchart/serial.js"></script>
    <script src="~/AdminPages/assets/pages/widget/amchart/light.js"></script>
    <script src="~/AdminPages/assets/pages/widget/amchart/pie.min.js"></script>
    <script src="https://www.amcharts.com/lib/3/plugins/export/export.min.js"></script>
    <!-- menu js -->
    <script src="~/AdminPages/assets/js/pcoded.min.js"></script>
    <script src="~/AdminPages/assets/js/vertical-layout.min.js "></script>
    <!-- custom js -->
    <script src="~/Content/JAVA/showImagePreview.js"></script>
    <script type="text/javascript" src="~/AdminPages/assets/pages/dashboard/custom-dashboard.js"></script>
    <script type="text/javascript" src="~/AdminPages/assets/js/script.js "></script>
    <script type="text/javascript">
        function reload() {
            Session["Admin_ID"] = null;
            Session["Admin_username"] = null;
            Session["Admin_Name"] = null;
        }
    </script>

    <script>
        $(document).ready(function () {
            function Contains(text_one, text_two) {
                if (text_one.indexOf(text_two) != -1)
                    return true;
            }
            $("#NewSearch").keyup(function () {
                var searchText = $("#NewSearch").val().toLowerCase();
                $(".NewSearch").each(function () {
                    if (!Contains($(this).text().toLowerCase(), searchText)) {
                        $(this).hide();
                    }
                    else {
                        $(this).show();
                    }
                });
            });
        });
    </script>
</body>

</html>
